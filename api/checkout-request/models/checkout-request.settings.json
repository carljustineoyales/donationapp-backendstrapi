{
  "connection": "default",
  "collectionName": "checkout_requests",
  "info": {
    "name": "Checkout"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "reference_id": {
      "type": "string"
    },
    "user_id": {
      "type": "biginteger"
    },
    "mode_of_payment": {
      "type": "string"
    },
    "amount": {
      "type": "float"
    },
    "currency": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "email": {
      "type": "email"
    },
    "campaign_id": {
      "type": "biginteger",
      "unique": true
    },
    "username": {
      "type": "string",
      "unique": true
    }
  }
}
